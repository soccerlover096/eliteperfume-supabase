############
# Secrets
############

# Postgres password - change this!
POSTGRES_PASSWORD=your-super-secret-db-password

# JWT secret - change this! (at least 32 characters)
JWT_SECRET=your-super-secret-jwt-token-with-at-least-32-characters

############
# Database
############

POSTGRES_HOST=postgres
POSTGRES_DB=postgres
POSTGRES_PORT=5432

############
# API Config
############

# External URL for your local Supabase instance
API_EXTERNAL_URL=http://localhost:8000
SITE_URL=http://localhost:3000

# Ports
KONG_HTTP_PORT=8000
KONG_HTTPS_PORT=8443
STUDIO_PORT=3001

# Database schemas to expose
PGRST_DB_SCHEMAS=public,storage,graphql_public

# JWT
JWT_EXPIRY=3600

############
# Auth
############

DISABLE_SIGNUP=false
ENABLE_EMAIL_SIGNUP=true
ENABLE_EMAIL_CONFIRMATIONS=false

# Email settings (optional - configure if you want email auth)
SMTP_HOST=
SMTP_PORT=
SMTP_USER=
SMTP_PASS=
SMTP_SENDER_NAME=ElitePerfume
SMTP_ADMIN_EMAIL=admin@eliteperfume.local

# Auth URLs
MAILER_URLPATHS_INVITE=/auth/v1/verify
MAILER_URLPATHS_CONFIRMATION=/auth/v1/verify
MAILER_URLPATHS_RECOVERY=/auth/v1/verify
MAILER_URLPATHS_EMAIL_CHANGE=/auth/v1/verify

ADDITIONAL_REDIRECT_URLS=

############
# Logs
############

LOGFLARE_API_KEY=your-logflare-api-key

############
# Storage
############

IMGPROXY_ENABLE_WEBP_DETECTION=true

